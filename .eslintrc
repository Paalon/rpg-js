{
  "extends": "eslint:recommended",
  "env" : {
    "browser": true,
    "node": true,
    "es6": true,
    },
  "ecmaFeatures": {
    "jsx": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
  },
  "rules": {
    "semi": 2,
    "arrow-body-style": 2,
    "arrow-parens": 0,
    "arrow-spacing": 2,
    "generator-star-spacing": 2,
    "no-useless-constructor": 0,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 0,
    "prefer-reflect": 1,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 0,
    "require-yield": 2,
    "template-curly-spacing": 2,
    "yield-star-spacing": 2,
    "no-unused-vars": 1,
    "no-console": 1
  }
}
